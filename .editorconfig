# See <https://editorconfig.org/>

# Root configuration (don't look further into parents)
root = true

# All files
[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.ssb]
trim_trailing_whitespace = false

[*.yml]
indent_size = 2